#algoritmo "trabalhando com tipos de vari√°veis"

var
    nome, sobrenome, nomeCompleto: literal
inicio
    Escreval ("Informe o seu nome: ")
    leia(nome)
    Escreval ("Informe o seu sobrenome: ")
    leia(sobrenome)
    nomeCompleto <- nome + " " + sobrenome
    Escreval (nomeCompleto)
fimalgoritmo
