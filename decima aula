algoritmo "Operador de escolha"

var
voto: literal

inicio
Escreval ("Digite o seu voto")
leia (voto)
Escolha voto
caso "20"
    Escreval ("Você votou em Jose Vitor")
caso "30"
    Escreval ("Você votou em Fallen")
caso "40"
    Escreval ("Você votou em Lincoln Lau")
outrocaso
    Escreval ("Você votou nulo")
fimescolha

fimalgoritmo
