algoritmo "Tabela Verdade"

var
i,j : inteiro

inicio
i <- 10
j <- 20
    // sim E nao = NAO
    // sim E sim = SIM
    // nao E nao = NAO
    // sim OU sim = SIM
    // nao OU nao = NAO
    // sim OU nao = SIM
se (i = 10) E (j = 20)
    Escreval ("Faça algo")
senao
    Escreval ("Não faça absolutamente nada")
fimse

fimalgoritmo
